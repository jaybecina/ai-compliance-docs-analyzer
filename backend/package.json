{
  "name": "ai-compliance-docs-analyzer-backend",
  "version": "1.0.0",
  "private": true,
  "type": "commonjs",
  "main": "dist/server.js",
  "scripts": {
    "dev": "nodemon",
    "build": "tsc -p tsconfig.json",
    "start": "node dist/server.js",
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config drizzle.config.ts",
    "db:migrate:safe": "ts-node -P tsconfig.json src/db/migrate.cli.ts",
    "db:seed": "ts-node -P tsconfig.json src/db/seed.cli.ts",
    "test": "jest",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.71.2",
    "@langchain/anthropic": "^1.3.7",
    "@pinecone-database/pinecone": "^6.1.3",
    "bcryptjs": "^3.0.2",
    "better-sqlite3": "^11.8.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "drizzle-orm": "^0.44.5",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^1.2.7",
    "multer": "^2.0.2",
    "pdf-parse": "^2.4.5",
    "zod": "^4.3.5"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/better-sqlite3": "^7.6.13",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.5",
    "@types/supertest": "^6.0.3",
    "drizzle-kit": "^0.31.4",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.3",
    "ts-jest": "^29.2.5",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
