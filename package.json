{
  "name": "ai-compliance-docs-analyzer",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "db:generate": "drizzle-kit generate --config drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config drizzle.config.ts",
    "db:migrate:safe": "ts-node -P backend/tsconfig.json backend/src/db/migrate.cli.ts",
    "db:studio": "drizzle-kit studio --config drizzle.config.ts",
    "db:seed": "ts-node -P backend/tsconfig.json backend/src/db/seed.cli.ts",
    "test": "vitest run",
    "test:backend:coverage": "vitest run --coverage",
    "test:client": "cd client && npm test",
    "test:client:coverage": "cd client && npm run test:coverage",
    "test:coverage": "npm run test:backend:coverage && npm run test:client:coverage",
    "dev:backend": "nodemon",
    "dev:frontend": "cd client && npm run dev",
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "build:frontend": "cd client && npm run build",
    "build:backend": "tsc -p backend",
    "build": "concurrently \"npm run build:backend\" \"npm run build:frontend\""
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "commonjs",
  "dependencies": {
    "bcryptjs": "^3.0.2",
    "@anthropic-ai/sdk": "^0.71.2",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@fontsource/roboto": "^5.2.9",
    "@langchain/anthropic": "^1.3.7",
    "@mui/icons-material": "^7.3.7",
    "@mui/material": "^7.3.7",
    "@pinecone-database/pinecone": "^6.1.3",
    "axios": "^1.13.2",
    "cors": "^2.8.5",
    "drizzle-orm": "^0.44.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.2",
    "langchain": "^1.2.7",
    "multer": "^2.0.2",
    "pdf-parse": "^2.4.5",
    "uuid": "^13.0.0",
    "zod": "^4.3.5",
    "better-sqlite3": "^11.8.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/multer": "^2.0.0",
    "@types/node": "^25.0.5",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^2.1.9",
    "drizzle-kit": "^0.31.4",
    "concurrently": "^8.2.2",
    "nodemon": "^3.1.11",
    "supertest": "^7.1.3",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3",
    "vitest": "^2.1.9"
  }
